package kr.hs.study.main;

import kr.hs.study.beans.HelloworldEn;

public class MainClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		HelloworldEn hello = new HelloworldEn();
		HelloworldKn hello2 = new HelloworldKn();
		
		callMethod(hello);
		callMethod2(hello2);
	}
	public static void callMethod(HelloworldEn hello) {
		hello.sayHello();
	}
	
	
	public static void callMethod2(HelloworldKn hello2) {
		hello2.sayHello();
	}
	
	

	
}
